            <script type="text/javascript">

    let webMapId = 3;

    let spinner = document.querySelector('#bridge-map-spinner-NGBSCizZPb');

    let selectedElements = [];
    let productsToShow = [];

    // On r√©cup√®re les options choisies pour les param√®tres de la carte
    let scrollWheelZoom = true;
    
    let isDisplayingZoomControl = true;
    let isDisplayingFullScreenControl = true;
    
    let isCenteringMap = true;
    
    let isDisplayingPopup = true;
    
    /**
    * Fonction appel√©e pour remplacer son √©quivalent document.ready jquery
    * @param {*} fn
    */
    if (!bridgeDocumentReady || typeof bridgeDocumentReady !== "function") {
        function bridgeDocumentReady (fn) {
            // see if DOM is already available
            if (document.readyState === "complete" || document.readyState === "interactive") {
                // call on next available tick
                setTimeout(fn, 10);
            } else {
                document.addEventListener("DOMContentLoaded", fn);
            }
        }
    }

    bridgeDocumentReady(function() {

        // On initialise la carte avec les options choisies
        window.mapService_NGBSCizZPb = new dncLeafletServices('bridge-map-NGBSCizZPb');
        window.mapService_NGBSCizZPb.mapoptions = {
            zoom: 13, // Niveau de zoom de d√©part
            centerlat: 48.4769792, // Latitude du centre au d√©part
            centerlon: 7.225344, // Longitude du centre au d√©part
            maptype: 'MAPTYPE_OPENSTREETMAPS', // Fond de carte
            gridSize: 0,
            maxZoom: '17', // Niveau de zoom √† partir duquel les points ne se regroupent pas
            scrollWheelZoom: scrollWheelZoom, // Bool√©en : peut-on zoomer √† la molette ?
            keyIgn: '9zm4vxq44jlp6ysq6j3uj0ld', // Clef n√©cessaire pour l'utilisation d'un fond de carte IGN
            isDisplayingZoomControl: false, // Affichage des boutons de zoom
            isDisplayingFullScreenControl: isDisplayingFullScreenControl // Affichage du bouton de plein √©cran
        };

        window.mapService_NGBSCizZPb.init();
        window.mapService_NGBSCizZPb.switchToWordpress();

        let itemLabels = document.querySelectorAll(".bridge-map-item-label");

        let items = document.querySelectorAll(".bridge-map-item");

        // Pour mieux afficher les intitul√©s des items s'ils sont longs
        let itemWidth= null;
        if (items.length > 0) {
            itemWidth = items[0].offsetWidth;
        }
        itemLabels.forEach((itemLabel) => {
            if (itemLabel.offsetWidth > itemWidth) {
                itemLabel.style.whiteSpace = "normal";
            }
        });

        // On place les boutons de contr√¥le et l'attribution Leaflet selon le template de la carte et l'option choisie dans les param√®tres de la carte
                            new L.Control.Zoom({ position: 'topright' }).addTo(window.mapService_NGBSCizZPb.mapobject);
                    window.mapService_NGBSCizZPb.mapobject.fullscreenControl.setPosition('topright');
                                window.mapService_NGBSCizZPb.mapobject.attributionControl.setPosition('bottomright');
                
        // Conteneur de la l√©gende
        let legend_NGBSCizZPb = document.querySelector('#bridge-map-legend-NGBSCizZPb');

        // On affiche ou non la l√©gende au chargement
        
        // Bouton d'affichage de la l√©gende
        let showLegendBtn_NGBSCizZPb = document.querySelector('#bridge-map-legend-show-NGBSCizZPb');
        // On l'affiche ou non selon l'option choisie
        
        // On affiche la l√©gende au clic sur le bouton
        showLegendBtn_NGBSCizZPb.addEventListener('click', (e) => {
            legend_NGBSCizZPb.style.display = 'block';
            showLegendBtn_NGBSCizZPb.style.display = 'none';
        });

        // Bouton pour cacher la l√©gende
        let hideLegendBtnNGBSCizZPb = document.querySelector('#bridge-map-legend-hide-NGBSCizZPb');

        // On cache la l√©gende au clic sur la croix (sauf si l'option pour la r√©afficher n'est pas choisie, pour √©viter qu'elle disparaisse pour de bon)
                    hideLegendBtnNGBSCizZPb.style.display = 'none';
        
        // Si les options choisies sont en m√™me temps de ne pas afficher la l√©gende et de ne pas afficher de bouton pour l'afficher, alors on charge directement tous les points
        

        let subItems = document.querySelectorAll(".bridge-map-subitem");
        let activeSubItems = document.querySelectorAll(".bridge-map-subitem-active")




        // On r√©cup√®re les items s√©lectionn√©s pour affichage
        subItems.forEach(subItem => {
            subItem.addEventListener('click', (e) => {
                 let subItemId = subItem.id.split('bridge-map-subitem-')[1];
                getSelectedElements(subItem, subItemId);

            });
        });

        // On affiche les √©l√©ments au chargement
        activeSubItems.forEach(activeSubItem => {
                let subItemId = activeSubItem.id.split('bridge-map-subitem-')[1];
                getSelectedElements(activeSubItem, subItemId);
        });

        items.forEach(item => {
            if (item.classList.contains('not-category')) {
                item.addEventListener('click', (e) => {
                    let itemId = item.id.split('bridge-map-item-')[1];
                    getSelectedElements(item, itemId);

                });

            }
        });

        // On injecte le KML si il a √©t√© renseign√© (notamment pour les d√©limitations)
        

        window.addEventListener('resize', windowResize());
        windowResize();



    });


    function windowResize (){
        jQuery('.bridge-map-legend').css(('max-height'),jQuery('.bridge-map-legend').css('min-height'));

    }

    /**
     * Ajoute ou retire les √©l√©ments s√©lectionn√©s ou d√©s√©lectionn√©s de la liste √† afficher
     * @param object item : l'√©l√©ment HTML cliqu√©
     * @param string itemId : l'id de l'item correspondant √† cet √©l√©ment
     */
    function getSelectedElements (item, itemId) {

        if (!item.classList.contains('active')) {
            item.classList.add('active');
            selectedElements.push(itemId);
        } else {
            item.classList.remove('active');
            selectedElements.splice(selectedElements.indexOf(itemId), 1);
        }

        window.mapService_NGBSCizZPb.hideallkmls();

        if (selectedElements.length > 0) {
            getProductsToShow(selectedElements);
        } else {
            // Si on a tout d√©s√©lectionn√©, on efface tous les rep√®res et trac√©s
            window.mapService_NGBSCizZPb.delallmarkers();
        }
    }

    /**
     * R√©cup√®re en AJAX les produits associ√©s aux items s√©lectionn√©s
     * @param array selectedElements : tableau contenant les id de tous les items s√©lectionn√©s
     */
    function getProductsToShow(selectedElements) {

        // Comme c'est un peu lent, on affiche un spinner en attendant le r√©sultat de la requ√™te AJAX pour √©viter des erreurs si l'utilisateur d√©s√©lectionne des items trop t√¥t
        // z-index tr√®s √©lev√© pour d√©passer celui des tiles de Leaflet
        spinner.style.display = 'flex';
        spinner.style.zIndex = 10000;

        jQuery.ajax({
            url: '/wp-admin/admin-ajax.php',
            type: 'GET',
            dataType: 'json',
            data: {
                action: 'webmap_get_products_to_show',
                lang: 'fr',
                activatedItems: selectedElements
            },
            success: (function (response) {
                if (response.success) {

                    if (response.data.message !== undefined) {
                        // Si certains items causent des erreurs, on n'interrompt pas l'affichage des autres mais on affiche l'erreur en console
                        productsToShow = response.data.data;

                        if (productsToShow.length !== 0) {
                            displayMarkers(productsToShow);
                            if(jQuery('#bridge-map-legend-mobile-NGBSCizZPb').length) {
                                UIkit.offcanvas('#bridge-map-legend-mobile-NGBSCizZPb').hide();
                            }

                        }

                        response.data.message.forEach(message => {
                            console.log(message);
                        });

                    } else {
                        productsToShow = response.data;

                        if (productsToShow.length !== 0) {
                            displayMarkers(productsToShow);
                            if(jQuery('#bridge-map-legend-mobile-NGBSCizZPb').length) {
                                UIkit.offcanvas('#bridge-map-legend-mobile-NGBSCizZPb').hide();
                            }

                        } else {
                            // S'il ne reste que des items qui n'ont aucun produit, on efface directement tous les rep√®res et trac√©s des items pr√©c√©dents
                            window.mapService_NGBSCizZPb.delallmarkers();
                            window.mapService_NGBSCizZPb.hideallkmls();

                            spinner.style.display = 'none';
                            spinner.style.zIndex = 0;
                        }
                    }
                    window.mapService_NGBSCizZPb.reDraw();
                } else {
                    console.log(response.data);
                }
            }),
            error: (function (response, xhr, errorThrown) {
                console.error('error', response);
                console.error('xhr', xhr);
                console.error('errorThrown', errorThrown);
                spinner.style.display = 'none';
                spinner.style.zIndex = 0;
            })
        });
    }

    /**
     * Affiche les marqueurs d'apr√®s la liste des produits associ√©s aux items s√©lectionn√©s
     * @param array productsToShow : tableau d'objets contenant les produits √† afficher avec les infos n√©cessaires
     */
    function displayMarkers(productsToShow) {

        let onclickMarker = function(id) {
            // on n'appelle le contenu que si on veut l'afficher
            let isPopupOpen = window.mapService_NGBSCizZPb.markers[id].marker.isPopupOpen();

            if (isDisplayingPopup && !isPopupOpen) {
                getPopupMarker(id);
            }

        };

        let onclickKml = function(id, e) {
            // On fixe ici le format selon le template
            let minWidth = formatPopup();
            let maxWidth = minWidth;

            let popup = L.popup({minWidth: minWidth, maxWidth: maxWidth});

            // on n'appelle le contenu que si on veut l'afficher
            let isPopupOpen = window.mapService_NGBSCizZPb.markers[id].KmlLayer.isPopupOpen();

            if (isDisplayingPopup && !isPopupOpen) {
                let map = window.mapService_NGBSCizZPb.mapobject;
                popup.setLatLng(e.latlng);
                getPopupKml(id, popup);
            }

        };

        // On compare les rep√®res actuels avec les produits √† afficher pour supprimer ceux qui ne sont plus n√©cessaires
        // (au lieu de tous les supprimer et tous les recr√©er √† chaque fois)
        let markerKeys = Object.keys(window.mapService_NGBSCizZPb.markers);

        markerKeys.forEach(key => {
            let isToShow = productsToShow.some(product => product.productCode == key);
            if (!isToShow) {
                window.mapService_NGBSCizZPb.delmarker(key);
            }
        });

        // On ajoute les marqueurs correspondants aux nouveaux produits s√©lectionn√©s
        productsToShow.forEach(product => {

            var mapMediaPath = 'https://www.rando-bruche.fr/wp-content/plugins/plugin-sit//assets/img/map-marker-icon.png';
            if(product.productMapMedia != null)
                mapMediaPath = 'https://bridgesrv-bruche.pylot.fr/media/' + product.productMapMedia;

            var popupcontent = null;
            if (isDisplayingPopup) {
                var popupcontent = '';
            }

            if (!window.mapService_NGBSCizZPb.markers[product.productCode]) {
                window.mapService_NGBSCizZPb.addmarker(
                    product.productCode,
                    product.productLat,
                    product.productLong,
                    product.productName,
                    mapMediaPath,
                    mapMediaPath,
                    popupcontent,
                    onclickMarker
                );
            }

            // affichage des kml, selon l'option choisie
            if (product.productKml != null && product.isDisplayingKmlPaths) {
                window.mapService_NGBSCizZPb.kmlStyleOptions.color = '#0461FE';

                window.mapService_NGBSCizZPb.addkml(
                    product.productKml,
                    product.productCode,
                    window.mapService_NGBSCizZPb.kmlStyleOptions,
                    false,
                    onclickKml
                );
            }



        });

        // Ici on ajuste le zoom aux points affich√©s, selon l'option choisie
        if (isCenteringMap) {
            setTimeout(function() {
                window.mapService_NGBSCizZPb.fitboundstomarkers();
            }, 250);
        }

        
        spinner.style.zIndex = 0;
    }

    /**
     * Appel√©e au clic sur un marqueur de la carte
     * @param number productCode : code produit correspondant au rep√®re cliqu√©
     */
    function getPopupMarker(productCode) {
        spinner.style.zIndex = 10000;
        return jQuery.ajax({
            url: '/wp-admin/admin-ajax.php',
            type: 'GET',
            dataType: 'json',
            data: {
                action: 'webmap_get_product_data',
                map: webMapId,
                lang: 'fr',
                product: productCode,
                popup: 'popup4'
            },
            success: (function (response) {
                let content = document.createElement('div');
                content.innerHTML = response.data;
                content.classList.add('popup-NGBSCizZPb');

                let popupWidth = formatPopup();

                window.mapService_NGBSCizZPb.markers[productCode].marker.bindPopup(content, {minWidth: popupWidth});
                window.mapService_NGBSCizZPb.markers[productCode].marker.openPopup();
                spinner.style.zIndex = 0;
            }),
            error: (function (response, xhr, errorThrown) {
                console.error('error', response);
                console.error('xhr', xhr);
                console.error('errorThrown', errorThrown);
            })
        })
    }

    /**
     * Appelle en AJAX les donn√©es du produit √† afficher, les place dans le pop-up cr√©√© et ouvre celui-ci
     *
     * @param number productCode : code produit correspondant au rep√®re cliqu√©
     * @param object popup : le pop-up √† afficher
     */
    function getPopupKml(productCode, popup) {
        jQuery.ajax({
            url: '/wp-admin/admin-ajax.php',
            type: 'GET',
            dataType: 'json',
            data: {
                action: 'webmap_get_product_data',
                map: webMapId,
                lang: 'fr',
                product: productCode,
                popup: 'popup4'
            },
            success: (function (response) {

                let content = document.createElement('div');
                content.innerHTML = response.data;
                content.classList.add('popup-NGBSCizZPb');

                popup.setContent(content);
                popup.openOn(window.mapService_NGBSCizZPb.mapobject);
            }),
            error: (function (response, xhr, errorThrown) {
                console.error('error', response);
                console.error('xhr', xhr);
                console.error('errorThrown', errorThrown);
            })
        })
    }

    /**
     * D√©termine le format du pop-up selon le template choisi
     * @return number
     */
    function formatPopup() {
        let popupWidth = 300;

                        popupWidth = 240;

                
        return popupWidth;
    }
</script>
